{
  "presets": [
    [
      "@babel/preset-env",
      {
        "debug": true,
        "targets": "> 0.25%, not dead"
      }
    ]
  ],
  "plugins": [
    [
      "@babel/plugin-transform-runtime",
      {
        "regenerator": true
      }
    ],
    "@babel/plugin-proposal-class-properties"
  ]
}
